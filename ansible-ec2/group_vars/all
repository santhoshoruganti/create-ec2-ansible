# EC2 instance variable

ec2:
  region: us-east-1
  credentials: santhosh-devops
  image: ami-05f27d4d6770a43d2
  #sg: AllowSSH
  security_group: sg-0c95c8d061ef871bf
  instance_type: t2.micro
  subnet: subnet-00ea2480cb39a0a04
  public_ip: yes
  tags:
    Name: Ec2Ansible
  volumes:
    - device_name: /dev/xvda
      volume_type: standard
      volume_size: 8
